<div class="sidebar-sticky min-vh-100">
  <nav class="nav flex-column">
    <% if user_signed_in? %>
       <span class="navbar-brand">Welcome back, <%= current_user.username.capitalize %></span>
    <ul class="nav flex-column text-secondary">
      <%= render 'navlinks' %>
    </ul>
  <% else %>
        <%= link_to "Sign In", new_user_session_path, class: "m-2 btn btn-secondary" %>
        <%= link_to "Sign Up", new_user_registration_path, class: "m-2 btn btn-secondary" %>      
  <% end %>
  </nav>
</div>
      